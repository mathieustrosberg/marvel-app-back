{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/characters",
      "methods": ["GET"],
      "dest": "src/index.js"
    },
    {
      "src": "/api/characters/(.*)",
      "methods": ["GET"],
      "dest": "src/index.js"
    },
    {
      "src": "/api/comics",
      "methods": ["GET"],
      "dest": "src/index.js"
    },
    {
      "src": "/api/comics/(.*)",
      "methods": ["GET"],
      "dest": "src/index.js"
    }
  ]
}
